    <androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/padding_default_16"
        android:layout_marginEnd="@dimen/padding_default_16"
        android:layout_marginTop="@dimen/padding_default_16"
        android:layout_marginBottom="@dimen/padding_default_16"
        android:background="@drawable/colores_gradientes_sky"
        >
        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/imgCircleExperiencias"
            android:layout_width="250dp"
            android:layout_height="250dp"
            android:layout_marginTop="@dimen/padding_default_16"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/ic_cloud"
            />

        <TextView
            android:id="@+id/txtInflableExperiencias"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8sp"
            android:textSize="22dp"
            android:textStyle="bold"
            android:text="Titulo"
            android:textColor="@color/color_surface"
            android:layout_marginTop="@dimen/padding_default_16"
            app:layout_constraintTop_toBottomOf="@id/imgCircleExperiencias"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            />

        <LinearLayout
            android:id="@+id/layoutValoracionExperiencias"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@id/txtInflableExperiencias"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00000000"
                android:layout_marginStart="@dimen/padding_default_16"
                >

                <ImageButton
                    android:id="@+id/btnCalificacionExperiencias1"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_star"
                    android:background="#00000000"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"/>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >

                <ImageButton
                    android:id="@+id/btnCalificacionExperiencias2"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_star"
                    android:background="#00000000"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"/>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00000000"
                >

                <ImageButton
                    android:id="@+id/btnCalificacionExperiencias3"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_star"
                    android:background="#00000000"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"/>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00000000"
                >

                <ImageButton
                    android:id="@+id/btnCalificacionExperiencias4"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_star"
                    android:background="#00000000"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"/>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#00000000"
                >

                <ImageButton
                    android:id="@+id/btnCalificacionExperiencias5"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_star"
                    android:background="#00000000"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"/>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>

        <TextView
            android:id="@+id/txtFechaExperiencias"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/txt_field_fecha"
            android:textSize="@dimen/padding_default_16"
            android:textColor="@color/color_surface"
            android:layout_gravity="center_vertical|end"
            android:layout_marginStart="@dimen/padding_default_16"
            android:layout_marginEnd="@dimen/padding_default_16"
            android:layout_marginTop="@dimen/common_padding_min_8"
            app:layout_constraintTop_toBottomOf="@id/layoutValoracionExperiencias"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="end"
            android:layout_marginTop="@dimen/common_padding_min_8"
            android:layout_marginEnd="@dimen/padding_default_16"
            android:paddingBottom="@dimen/padding_default_16"
            app:layout_constraintTop_toBottomOf="@id/txtFechaExperiencias"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/btnApartarExperiencias"
                android:layout_width="100dp"
                android:layout_height="40dp"
                android:background="@drawable/colores_gradientes_fire"
                android:layout_marginEnd="@dimen/common_padding_min_8"
                >
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Apartame"
                    android:textColor="@color/color_surface"
                    android:textStyle="bold"
                    android:textSize="@dimen/padding_default_16"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    />
            </androidx.constraintlayout.widget.ConstraintLayout>


            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/btnCallExperiencias"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:src="@drawable/ic_flecha_siguiente"
                android:layout_marginEnd="@dimen/common_padding_min_8"
                />
            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/btnWhatsExperiencias"
                android:layout_width="40dp"
                android:layout_height="40dp"/>

        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
